{
  "name": "l7editor-vscode-extension",
  "displayName": "l7editor-vscode-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage: json",
    "onLanguage:geojson"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "l7editor-vscode-extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "a",
        "title": "View Editor",
        "category": "Geo"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "a",
          "when": "resourceFilename =~ /.*(.json|.geojson)/",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run build && npm run package",
    "dev": "webpack-dev-server -c webpack.dev.js",
    "build": "webpack -c webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.8.0",
    "@vscode/test-electron": "^2.3.4",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@antv/l7": "^2.19.5",
    "@antv/l7-editor": "^1.1.4",
    "antd": "^5.10.2"
  }
}
